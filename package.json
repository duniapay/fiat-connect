{
  "name": "fiatconnect-api",
  "version": "0.0.1",
  "description": "DUNIA PAYMENT implementation of FiatConnect API",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i -g pm2 && pm2 install pm2-logrotate",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --loglevel error --write \"./src/**/*.ts\"",
    "lint": "eslint --ext=.tsx,.ts --fix src/",
    "dev": "./scripts/dev.sh",
    "start": "./scripts/start.sh",
    "test": "jest --config=jest.config.js",
    "test:e2e": "jest --config=jest-e2e.config.js",
    "dev:server": "NODE_ENV=localhost nodemon  -r dotenv/config --watch './**/*.ts' --exec 'ts-node' --files ./src/index.ts --auth-config-option alfajores --session-secret poiii",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "prettier": "@valora/prettier-config",
  "license": "MIT",
