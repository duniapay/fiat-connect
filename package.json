{
   "name": "fiatconnect-api",
   "version": "0.0.1",
   "description": "An example implementation of a FiatConnect-compliant API",
   "main": "index.js",
   "engines": {
      "node": "^16"
   },
   "scripts": {
      "build": "tsc",
      "format": "prettier --loglevel error --write \"./src/**/*.ts\"",
      "lint": "eslint --ext=.tsx,.ts --fix src/",
      "start": "ts-node src/index.ts --auth-config-option alfajores --session-secret poiii",
      "dev": "./scripts/dev.sh",
      "test": "jest",
      "dev:server": "NODE_ENV=localhost nodemon --watch './**/*.ts' --exec 'ts-node' --files ./src/index.ts --auth-config-option alfajores --session-secret poiii",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "prettier": "@valora/prettier-config",
   "license": "MIT",
   "dependencies": {
      "@celo/utils": "^1.5.2",
      "@fiatconnect/fiatconnect-types": "^6.1.0",
      "@types/express": "^4.17.13",
      "@types/node": "^17.0.21",
      "@types/yargs": "^17.0.10",
      "@typescript-eslint/eslint-plugin": "^5.12.1",
      "@valora/eslint-config-typescript": "^0.0.1",
      "@valora/prettier-config": "^0.0.1",
      "ajv": "^8.11.0",
      "api-contract-validator": "^2.2.8",
      "axios": "^0.27.2",
      "chai": "^4.3.6",
      "dotenv": "^16.0.1",
      "dunia-types": "^1.0.2",
      "eslint": "^8.9.0",
      "eslint-plugin-import": "^2.25.4",
      "eslint-plugin-react": "^7.28.0",
      "eslint-plugin-react-hooks": "^4.3.0",
      "eslint-plugin-react-native": "^4.0.0",
      "ethers": "^5.6.9",
      "express": "^4.17.2",
      "express-idempotency": "^1.0.5",
      "express-session": "^1.17.3",
      "pg": "^8.4.0",
      "prettier": "^2.7.1",
      "redis": "^4.2.0",
      "reflect-metadata": "^0.1.13",
      "siwe": "^1.1.6",
      "typeorm": "0.3.7",
      "typescript": "^4.7.4",
      "uuid": "^8.3.2",
      "yargs": "^17.5.1"
   },
   "devDependencies": {
      "@types/chai": "^4.3.1",
      "@types/express-session": "^1.17.4",
      "@types/jest": "^27.4.1",
      "@types/node": "^16.11.10",
      "@types/node-fetch": "2",
      "@types/supertest": "^2.0.11",
      "@types/uuid": "^8.3.4",
      "@types/yargs": "^17.0.10",
      "@typescript-eslint/eslint-plugin": "^5.29.0",
      "@valora/eslint-config-typescript": "^1.0.0",
      "@valora/prettier-config": "^0.0.1",
      "eslint": "^8.18.0",
      "eslint-plugin-chai-expect": "^3.0.0",
      "jest": "^27.5.1",
      "node-fetch": "2",
      "nodemon": "^2.0.19",
      "supertest": "^6.2.2",
      "ts-jest": "^27.1.3",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
