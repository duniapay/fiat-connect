    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "ethers": "^5.6.9",
    "express": "^4.17.2",
    "express-idempotency": "^1.0.5",
    "express-session": "^1.17.3",
    "pg": "^8.4.0",
    "prettier": "^2.7.1",
